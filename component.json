{
	name: "board",
	element: "div",
	state: {
		width: 3,
		height: 3
	},
	children: [
		{
			element: "div",
			class: "row",
			repeat: {
				array: this.state.height
			},
			children: [
				{
					element: "square",
					repeat: {
						array: this.state.width
					},
					args: {
						id: function(){return this.parent.repeat.index * 3 + this.repeat.index}
					}
				}
			]
		}
	]
}
