{
	name: "tictactoe",
	title: "Tic Tac Toe",
	state: {
		user: null
	},
	methods: {
		login: function(){},
		loadUser: function(){
			this.$api.get("api/user").then((response)=> {
				this.$state.set("user", response.data)
			})
		}
	},
	init: {
		this.loadUser()
	},
	routes: [
		{
			name: "home",
			path: "/",
			component: "board",
			children: [],
			meta: {
				permalink: "https://"
			}
		}
	],
	class: "bodyclass",
	children: [
		{
			element: "nav",
			class: "mainmenu",
			children: [
				{
					element: "img",
					class: "nav-img",
					src: "/img/logo.png"
				},
				{
					element: "ul",
					class: "nav-menu",
					children: [
						{
							element: "li",
							class: function(){return this.}		
						}
					]
				}
			]
		},
		{
			element: "router"
		}	
	]
}
